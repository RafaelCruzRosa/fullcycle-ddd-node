{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "strict": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
  },
}